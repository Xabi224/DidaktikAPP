<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">

        <OnClick motion:targetId="@+id/btnGarbi"/>
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_marginEnd="16dp"
            android:layout_height="55dp"
            android:layout_marginTop="552dp"
            android:layout_marginRight="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="55dp"
            android:id="@+id/imageCafe" />
        <Constraint
            android:layout_height="55dp"
            android:layout_marginBottom="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageLata"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            android:layout_height="55dp"
            android:layout_marginBottom="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/Arrautzak"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            android:layout_marginEnd="100dp" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imagePapera"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="552dp"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="100dp"
            motion:layout_constraintHorizontal_bias="1.0" />
        <Constraint
            android:layout_height="55dp"
            android:layout_marginBottom="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="16dp"
            android:id="@+id/imageSagarra" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:id="@+id/imagePlatanoa"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="552dp"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:id="@+id/btnGarbi" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="6dp"
            android:layout_marginEnd="8dp"
            android:id="@+id/btnNext" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="55dp"
            android:id="@+id/imageKristal_botila"
            android:layout_marginStart="100dp" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="55dp"
            android:id="@+id/imagePlastiko_botila"
            android:layout_marginStart="100dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="552dp"
            motion:layout_constraintVertical_bias="0.0" />
    </ConstraintSet>
</MotionScene>