<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".merkatu.MerkatuActivity">

    <ImageView
        android:id="@+id/imageErroskilak"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toTopOf="@+id/btnErroxkilak"
        app:layout_constraintEnd_toStartOf="@+id/imagePiperrak"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/GalderaMerkatu"
        app:srcCompat="@drawable/erroskilak" />

    <ImageView
        android:id="@+id/imagePiperrak"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="145dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/btnPiperrak"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageErroskilak"
        app:layout_constraintTop_toBottomOf="@+id/GalderaMerkatu"
        app:srcCompat="@drawable/piperrak" />

    <Button
        android:id="@+id/btnErroxkilak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="111dp"
        android:backgroundTint="#7CE3C5"
        android:text="erroxkilak"
        app:layout_constraintBottom_toTopOf="@+id/btnIndabak"
        app:layout_constraintEnd_toStartOf="@+id/btnPiperrak"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageErroskilak" />

    <Button
        android:id="@+id/btnAlfombra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="102dp"
        android:backgroundTint="#7CE3C5"
        android:text="Alfombra"
        app:layout_constraintBottom_toTopOf="@+id/btnYogurtak"
        app:layout_constraintEnd_toStartOf="@+id/btnZapatila"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageAlfombra" />

    <Button
        android:id="@+id/btnZapatila"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="107dp"
        android:backgroundTint="#7CE3C5"
        android:text="Zapatilak"
        app:layout_constraintBottom_toTopOf="@+id/btnTomateak"
        app:layout_constraintEnd_toStartOf="@+id/btnTxakolina"
        app:layout_constraintStart_toEndOf="@+id/btnAlfombra"
        app:layout_constraintTop_toBottomOf="@+id/btnArtoa"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnTxakolina"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#7CE3C5"
        android:text="Txakolina"
        app:layout_constraintBottom_toTopOf="@+id/imageAzenarioa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnZapatila"
        app:layout_constraintTop_toBottomOf="@+id/btnMugikorrak" />

    <ImageView
        android:id="@+id/imageAzenarioa"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="11dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/btnAzenarioa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/btnZapatila"
        app:layout_constraintTop_toBottomOf="@+id/btnTxakolina"
        app:srcCompat="@drawable/azenarioa" />

    <Button
        android:id="@+id/btnPiperrak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#7CE3C5"
        android:text="Piperrak"
        app:layout_constraintBottom_toTopOf="@+id/imageArtoa"
        app:layout_constraintEnd_toStartOf="@+id/btnDetergentea"
        app:layout_constraintStart_toEndOf="@+id/btnErroxkilak"
        app:layout_constraintTop_toBottomOf="@+id/imagePiperrak" />

    <ImageView
        android:id="@+id/imageAlfombra"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/btnAlfombra"
        app:layout_constraintEnd_toStartOf="@+id/imageArtoa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnIndabak"
        app:srcCompat="@drawable/alfonbra" />

    <Button
        android:id="@+id/btnYogurtak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="37dp"
        android:backgroundTint="#7CE3C5"
        android:text="Yogurtak"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnTomateak"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAlfombra" />

    <Button
        android:id="@+id/btnAzenarioa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="37dp"
        android:backgroundTint="#7CE3C5"
        android:text="Azenarioa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnTomateak"
        app:layout_constraintTop_toBottomOf="@+id/imageAzenarioa" />

    <Button
        android:id="@+id/btnTomateak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="37dp"
        android:backgroundTint="#7CE3C5"
        android:text="Tomateak"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnAzenarioa"
        app:layout_constraintStart_toEndOf="@+id/btnYogurtak"
        app:layout_constraintTop_toBottomOf="@+id/btnZapatila" />

    <ImageView
        android:id="@+id/imageArtoa"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="141dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/btnArtoa"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPiperrak"
        app:srcCompat="@drawable/artoa" />

    <Button
        android:id="@+id/btnIndabak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#7CE3C5"
        android:text="Indabak"
        app:layout_constraintBottom_toTopOf="@+id/imageAlfombra"
        app:layout_constraintEnd_toStartOf="@+id/btnArtoa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnErroxkilak" />

    <Button
        android:id="@+id/btnArtoa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="102dp"
        android:backgroundTint="#7CE3C5"
        android:text="Artoa"
        app:layout_constraintBottom_toTopOf="@+id/btnZapatila"
        app:layout_constraintEnd_toStartOf="@+id/btnMugikorrak"
        app:layout_constraintStart_toEndOf="@+id/btnIndabak"
        app:layout_constraintTop_toBottomOf="@+id/imageArtoa" />

    <Button
        android:id="@+id/btnMugikorrak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="102dp"
        android:backgroundTint="#7CE3C5"
        android:text="Mugikorra"
        app:layout_constraintBottom_toTopOf="@+id/btnTxakolina"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnArtoa"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnDetergentea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="186dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#7CE3C5"
        android:text="Detergentea"
        app:layout_constraintBottom_toTopOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnPiperrak"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/GalderaMerkatu"
        android:layout_width="283dp"
        android:layout_height="0dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="29dp"
        android:fontFamily="sans-serif"
        android:text="Zer produktu saltzen dira azokan?"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/imagePiperrak"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/btnMugikorrak"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageArtoa"
        app:layout_constraintTop_toBottomOf="@+id/btnDetergentea"
        app:srcCompat="@drawable/mugikorra" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="114dp"
        android:layout_height="98dp"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/indabak"
        tools:layout_editor_absoluteY="243dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="111dp"
        android:layout_height="98dp"
        android:layout_marginStart="148dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/zapatila"
        tools:layout_editor_absoluteY="393dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="51dp"
        android:layout_height="100dp"
        android:layout_marginEnd="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/txakolina"
        tools:layout_editor_absoluteY="389dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/detergentea"
        tools:layout_editor_absoluteY="54dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="120dp"
        android:layout_height="110dp"
        android:layout_marginBottom="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/tomateak"
        tools:layout_editor_absoluteX="139dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="88dp"
        android:layout_height="94dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/yogurta"
        tools:layout_editor_absoluteY="544dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>